@model IEnumerable<Post>

@{

}

<h2>Home page</h2>
<img src="~/content/static/350x150.jpg" />
<div>
    <ul>
        @foreach (var post in Model)
        {
            <li>
                <a asp-controller="Home"
                   asp-action="Post"
                   asp-route-id="@post.Id">
                    @post.Title
                </a>
            </li>
        }
    </ul>
</div>